<?xml version="1.0" encoding="UTF-8"?>

<!--
    Document   : FontAndColors.xml
    Created on : 12 April 2011, 9:58 PM
    Author     : Jacqui Hawkins
    Description:
        Defines fonts and colours for syntax highlighter.

        Reference:
        http://wiki.netbeans.org/How_to_create_support_for_a_new_language
-->

<!DOCTYPE fontscolors PUBLIC
    "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN"
    "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <fontcolor name="name" default="string"/>
    <fontcolor name="class" default="keyword" foreColor="magenta"/>
    <fontcolor name="comment" default="comment"/>
    <fontcolor name="compiler-directive" default="comment" foreColor="009900"/>
    <fontcolor name="cpp-code" default="comment" foreColor="008866"/>
    <fontcolor name="preprocessor-directive" default="comment" foreColor="009900"/>
    <fontcolor name="error" default="error"/>
    <fontcolor name="javadoc-comment" default="comment" foreColor="663399"/>
    <fontcolor name="identifier" default="identifier"/>
    <fontcolor name="javadoc-tag" default="comment" foreColor="996699">
        <font style="bold" />
    </fontcolor>
    <fontcolor name="keyword" default="keyword"/>
    <fontcolor name="literal" default="number" />
    <fontcolor name="local-declaration" default="keyword" foreColor="9999FF"/>
    <fontcolor name="method-declaration" default="method" foreColor="73A892">
        <font style="bold" />
    </fontcolor>
    <fontcolor name="number" default="number"/>
    <fontcolor name="operator" default="operator"/>
    <fontcolor name="separator" default="separator"/>
    <fontcolor name="string" default="string"/>
    <fontcolor name="var-declaration" default="keyword" foreColor="9999FF"/>
    <fontcolor name="whitespace" default="whitespace"/>
    
</fontscolors>
